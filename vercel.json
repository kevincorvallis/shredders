{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sfo1"],
  "crons": [
    {
      "path": "/api/scraper/run",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/check-weather-alerts",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/check-powder-alerts",
      "schedule": "0 6 * * *"
    }
  ]
}
